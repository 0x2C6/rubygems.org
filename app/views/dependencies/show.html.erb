<% @title = params[:rubygem_id] %>
<% @subtitle = @latest_version.try(:slug) %>

<% if @dependencies.present? && @latest_version.indexed %>
  <div class="l-full--l">
    <%= render partial: "dependencies/dependencies", locals: { scope: "runtime" } %>
    <%= render partial: "dependencies/dependencies", locals: { scope: "development" } %>
  </div>
<% end %>
